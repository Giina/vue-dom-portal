    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-espower": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "buble": "^0.14.0",
    "conventional-changelog-cli": "^1.1.1",
